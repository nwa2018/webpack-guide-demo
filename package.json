  "scripts": {
    "getstart": "npx webpack demo/getstart/src/index.js demo/getstart/dist/bundle.js",
    "getstart2": "npx webpack --config ./demo/getstart2/webpack.config.js",
    "asset": "npx webpack --config ./demo/asset/webpack.config.js",
